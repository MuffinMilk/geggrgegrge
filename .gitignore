        canvas.style.height = "100%";
      }

      createUnityInstance(document.querySelector("#unity-canvas"), {
        dataUrl:
          "https://pub-2d868f15a1824cc8a2ec51069ed41c6f.r2.dev/ultrakill.data.unityweb",
